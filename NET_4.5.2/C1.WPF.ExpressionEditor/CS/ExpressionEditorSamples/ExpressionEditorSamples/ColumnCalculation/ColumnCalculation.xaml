<UserControl x:Class="C1ExpressionEditorSample.ColumnCalculation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:C1ExpressionEditorSample"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="800">
    <Grid>
        <c1:C1TabControl>
            <c1:C1TabItem Header="Integration C1FlexGrid">
                <local:C1FlexGridIntegration />
            </c1:C1TabItem>
            <c1:C1TabItem Header="Integration C1DataGrid">
                <local:C1DataGridIntegration />
            </c1:C1TabItem>
            <c1:C1TabItem Header="Integration DataGrid">
                <local:DataGridIntegration />
            </c1:C1TabItem>
        </c1:C1TabControl>
    </Grid>
</UserControl>
