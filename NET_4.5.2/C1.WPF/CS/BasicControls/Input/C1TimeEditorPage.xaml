<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="BasicControls.DemoTimeEditor" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:helpers="clr-namespace:Helpers" xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/BasicControls.4;component/CommonResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <!-- The control -->
    <Border>
        <c1:C1TimeEditor Name="timeEditor" Format="TimeSpan" AllowNull="True" Value="{x:Null}" Height="25" VerticalAlignment="Center" HorizontalAlignment="Center" />
    </Border>
</UserControl>