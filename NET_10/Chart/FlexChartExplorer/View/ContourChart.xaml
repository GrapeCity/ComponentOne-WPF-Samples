<UserControl x:Class="FlexChartExplorer.ContourChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FlexChartExplorer"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <c1:FlexChart x:Name="chart" Loaded="ChartLoaded" Header="Contour Plot" ToolTipContent="">
            <c1:FlexChart.HeaderStyle>
                <c1:ChartStyle FontSize="20" />
            </c1:FlexChart.HeaderStyle>
        </c1:FlexChart>
    </Grid>
</UserControl>