<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Localization.Views.TabViews"
             xmlns:vm="clr-namespace:Localization.ViewModel"
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml" 
             x:Class="Localization.Views.TabViews.GanttViewTab"
             xmlns:GanttView="clr-namespace:C1.GanttView;assembly=C1.WPF.GanttView"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <UserControl.Resources>
        <vm:GanttViewModel x:Key="GanttViewData"/>
    </UserControl.Resources>
    <Grid DataContext="{StaticResource GanttViewData}">
        <c1:C1GanttView x:Name="GV" >
        </c1:C1GanttView>
    </Grid>
</UserControl>
