<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <StartupObject>FlexGridExplorer.App</StartupObject>
    <SignAssembly>false</SignAssembly>
    <Company>GrapeCity, Inc.</Company>
    <Authors>GrapeCity, Inc.</Authors>
    <Platforms>AnyCPU;x86</Platforms>
    <ApplicationIcon>C1-ball.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Images\Album.png" />
    <None Remove="Images\Artist.png" />
    <None Remove="Images\Collapsed.png" />
    <None Remove="Images\default.png" />
    <None Remove="Images\Expanded.png" />
    <None Remove="Images\input_form.png" />
    <None Remove="Images\retro_TV_filled-50_light.png" />
    <None Remove="Images\Song.png" />
    <None Remove="Images\star.png" />
    <None Remove="Properties\Licenses.licx" />
    <None Remove="Resources\data.zip" />
    <None Remove="Resources\Roboto-Regular.ttf" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Images\Album.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\Artist.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\Collapsed.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\default.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\Expanded.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\input_form.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\retro_TV_filled-50_light.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\Song.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\star.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Resources\data.zip" />
    <PackageReference Include="C1.DataCollection.BindingList" Version="1.0.20203.32" />
    <PackageReference Include="C1.WPF.Accordion">
      <Version>5.0.20203.29</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Core">
      <Version>5.0.20203.29</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.DataPager">
      <Version>5.0.20203.29</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.DateTimeEditors">
      <Version>5.0.20203.29</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Grid">
      <Version>5.0.20203.29</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Input">
      <Version>5.0.20203.29</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Ribbon">
      <Version>5.0.20203.29</Version>
    </PackageReference>
    <Resource Include="Resources\Roboto-Regular.ttf" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
    <Compile Update="Resources\AppResources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>AppResources.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Resources\AppResources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>AppResources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>
  <ItemGroup>
    <Page Update="Themes\generic.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
    <Page Update="View\CellFreezing.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
