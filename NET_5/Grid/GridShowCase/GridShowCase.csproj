<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <SignAssembly>false</SignAssembly>
    <Company>GrapeCity, Inc.</Company>
    <Authors>GrapeCity, Inc.</Authors>
    <Platforms>AnyCPU;x86</Platforms>
    <ApplicationIcon>c1.ico</ApplicationIcon>
    <AssemblyName>GridShowCase</AssemblyName>
    <RootNamespace>GridShowCase</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Resources\Images\Columns.png" />
    <None Remove="Resources\Images\ConditionalFormating.png" />
    <None Remove="Resources\Images\csv.png" />
    <None Remove="Resources\Images\datetime.png" />
    <None Remove="Resources\Images\Excel.png" />
    <None Remove="Resources\Images\Filter.png" />
    <None Remove="Resources\Images\Html.png" />
    <None Remove="Resources\Images\Ico-Filters.png" />
    <None Remove="Resources\Images\noflag.png" />
    <None Remove="Resources\Images\Text.png" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Resources\Images\Columns.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\ConditionalFormating.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\csv.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\datetime.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\Excel.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\Filter.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\Html.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\Ico-Filters.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\noflag.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\Text.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="C1.DataCollection" Version="1.0.20212.57" />
    <PackageReference Include="C1.WPF.Chart">
      <Version>5.0.20212.100</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.ColorPicker">
      <Version>5.0.20212.100</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Core">
      <Version>5.0.20212.100</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.DataCollection" Version="1.0.20212.57" />
    <PackageReference Include="C1.WPF.Grid">
      <Version>5.0.20212.100</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Input">
      <Version>5.0.20212.100</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Menu">
      <Version>5.0.20212.100</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Ribbon">
      <Version>5.0.20212.100</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.TabControl">
      <Version>5.0.20212.100</Version>
    </PackageReference>
    <PackageReference Include="Newtonsoft.Json">
      <Version>12.0.3</Version>
    </PackageReference>
    <PackageReference Include="System.ComponentModel.Annotations">
      <Version>4.7.0</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.ja.resx">
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>
</Project>