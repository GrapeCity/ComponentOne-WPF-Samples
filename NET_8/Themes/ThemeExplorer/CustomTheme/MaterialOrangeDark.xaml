<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Color & Brush used to build Base brushes -->
    <Color x:Key="PrimaryColor">#c98100</Color>
    <Color x:Key="BackgroundColor">#2d2d2d</Color>
    <Color x:Key="OnSurfaceColor">White</Color>
    <SolidColorBrush x:Key="MenuHighlightBrush" Color="{StaticResource PrimaryColor}" Opacity="0.04"/>
    <SolidColorBrush x:Key="SubtotalBackground" Color="#ed9900" />
    <Color x:Key="SelectedColor">#653e00</Color>

    <!-- C1HyperLinkButton Foreground-->
    <SolidColorBrush x:Key="HyperLinkForeground" Color="#c98100" />
    <SolidColorBrush x:Key="IndicatorBrush" Color="#c98100" />

    <SolidColorBrush x:Key="SliderThumb.Pressed.Background" Color="#ffddb7"/>
    <SolidColorBrush x:Key="SliderThumb.Pressed.Border" Color="#a66a00"/>

    <SolidColorBrush x:Key="FocusBrush" Color="{StaticResource SelectedColor}"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource PrimaryColor}"/>

</ResourceDictionary>