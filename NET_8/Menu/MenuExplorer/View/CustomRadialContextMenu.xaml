    <UserControl.Resources>
        <local:StringToPathConverter x:Key="StringToPathConverter"/>
        <Style TargetType="ContentControl" x:Key="iconStyle">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ContentControl">
                        <Grid Height="18" Width="18" Margin="-5" >
                            <Path  Data="{TemplateBinding Content, Converter={StaticResource StringToPathConverter}}" Fill="{Binding Foreground, RelativeSource={RelativeSource TemplatedParent}, Mode=OneWay}"  Stretch="Uniform"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

    </UserControl.Resources>
    <Grid>
        <c1:C1RichTextBox x:Name="rtb"  BorderThickness="2" BorderBrush="DarkGray"
                     RequestNavigate="rtb_RequestNavigate"
                     MouseLeftButtonDown="rtb_PointerPressed"
