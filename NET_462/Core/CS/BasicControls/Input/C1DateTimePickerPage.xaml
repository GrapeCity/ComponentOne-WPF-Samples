<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="BasicControls.DemoDateTimePicker" mc:Ignorable="d" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:helpers="clr-namespace:Helpers" xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/BasicControls.4;component/CommonResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <!-- The control -->
    <Border>
        <c1:C1DateTimePicker x:Name="picker" AllowNull="True" DateTime="{x:Null}" Height="25" VerticalAlignment="Center" HorizontalAlignment="Center" />
    </Border>
</UserControl>