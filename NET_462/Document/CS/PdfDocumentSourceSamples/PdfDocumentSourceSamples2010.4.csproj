<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <OutputType>WinExe</OutputType>
    <RootNamespace>PdfDocumentSourceSamples</RootNamespace>
    <AssemblyName>PdfDocumentSourceSamples.4</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWPF>true</UseWPF>
    <ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System.Printing" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Properties\Licenses.licx" />
    <EmbeddedResource Include="Resources\DefaultDocument.pdf" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Resources\DocumentWarning.png" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="C1.Xaml.WPF">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.BarCode">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.Bitmap">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.DateTimeEditors">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.Document">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.DX">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.FlexChart">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.FlexViewer">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.ImportServices">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
  </ItemGroup>
</Project>