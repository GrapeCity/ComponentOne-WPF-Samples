<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <OutputType>WinExe</OutputType>
    <RootNamespace>FinancialChartExplorer</RootNamespace>
    <AssemblyName>FinancialChartExplorer.4</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWPF>true</UseWPF>
    <ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="Properties\licenses.licx" />
    <AppDesigner Include="Properties\" />
    <EmbeddedResource Include="Resources\box.json" />
    <EmbeddedResource Include="Resources\fb.json" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Resources\C1.png" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Resources\box-annotations.json" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="C1.Xaml.WPF">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.DX">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.FinancialChart">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.FlexChart">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
  </ItemGroup>
</Project>