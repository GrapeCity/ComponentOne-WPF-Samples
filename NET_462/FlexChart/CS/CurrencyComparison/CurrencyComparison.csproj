<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <OutputType>WinExe</OutputType>
    <RootNamespace>CurrencyComparison</RootNamespace>
    <AssemblyName>CurrencyComparison</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
  <ItemGroup>
    <AppDesigner Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Properties\licenses.licx" />
    <Resource Include="Resources\CurrencyHistory.csv">
      <CopyToOutputDirectory>
      </CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\ImgLegend.png" />
    <Resource Include="Resources\Img_WaterMark_C1Logo.png" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="C1.Xaml.WPF">
      <Version>4.6.20252.900</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.DX">
      <Version>4.6.20252.900</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.FlexChart">
      <Version>4.6.20252.900</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
  </ItemGroup>
</Project>