<OlapViews>
  
  <C1OlapPage id="Product vs Country">
    <C1OlapEngine>
      <Fields>
        <Field name="Sales" subtotal="Sum" format="n0" />
        <Field name="OrderDate" subtotal="Count" />
        <Field name="Product" subtotal="Count" />
        <Field name="Customer" subtotal="Count" />
        <Field name="Country" subtotal="Count" />
        <Field name="Employee" subtotal="Count" />
        <Field name="Category" subtotal="Count" />
      </Fields>
      <RowFields>
        <Field name="Product" />
      </RowFields>
      <ColumnFields>
        <Field name="Country" />
      </ColumnFields>
      <ValueFields>
        <Field name="Sales" />
      </ValueFields>
    </C1OlapEngine>
    <C1OlapChart ChartType="Bar" ColorGeneration="Office" ShowTitle="True" ShowLegend="Automatic" ShowGridLines="True" Stacked="True" />
  </C1OlapPage>

