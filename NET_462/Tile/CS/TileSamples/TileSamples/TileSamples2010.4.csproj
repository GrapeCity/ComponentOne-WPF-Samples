<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <OutputType>WinExe</OutputType>
    <RootNamespace>TileSamples</RootNamespace>
    <AssemblyName>TileSamples.4</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWPF>true</UseWPF>
    <ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
    <DefineConstants>CLR40;SL_AUTOMATION</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="Properties\licenses.licx" />
    <AppDesigner Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\TileSamplesLib\TileSamplesLib2010.4.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\readme.txt">
      <Link>readme.txt</Link>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="C1.Xaml.WPF">
      <Version>4.6.20252.900</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.Tile">
      <Version>4.6.20252.900</Version>
    </PackageReference>
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
  </ItemGroup>
</Project>