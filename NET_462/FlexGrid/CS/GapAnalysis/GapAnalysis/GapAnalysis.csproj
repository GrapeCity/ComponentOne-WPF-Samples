<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <OutputType>WinExe</OutputType>
    <RootNamespace>GapAnalysis</RootNamespace>
    <AssemblyName>GapAnalysis</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWindowsForms>true</UseWindowsForms>
    <UseWPF>true</UseWPF>
    <ImportWindowsDesktopTargets>true</ImportWindowsDesktopTargets>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="Properties\licenses.licx" />
    <AppDesigner Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="FormStyle\FormFields.txt" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="C1.Xaml.WPF">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.DateTimeEditors">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.FlexGrid">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="C1.Xaml.WPF.FlexGridFilter">
      <Version>4.6.20251.887</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="5.0.0" />
    <PackageReference Include="System.Data.DataSetExtensions" Version="4.5.0" />
  </ItemGroup>
</Project>