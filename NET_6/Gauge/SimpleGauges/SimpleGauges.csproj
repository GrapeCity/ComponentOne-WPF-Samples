    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <ApplicationIcon>C1-ball.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <EmbeddedResource Include="SimpleGauges.gclicx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="C1.WPF.Core">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Gauge">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Rendering">
      <Version>7.0.20233.662</Version>
    </PackageReference>
  </ItemGroup>
</Project>