<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <ApplicationIcon>C1-ball.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="C1-ball.ico" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Strings\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.ja.resx">
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Strings\Resources.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <Page Update="View\C1MultiSelect.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
    <Page Update="View\DemoMaskedTextBox.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
    <Page Update="View\DemoTagEditor.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="InputExplorer.gclicx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="C1.WPF.Accordion">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.ColorPicker">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Core">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.DateTimeEditors">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Docking">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Input">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.ListView">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.Menu">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.PropertyGrid">
      <Version>7.0.20233.662</Version>
    </PackageReference>
    <PackageReference Include="C1.WPF.TabControl">
      <Version>7.0.20233.662</Version>
    </PackageReference>
  </ItemGroup>
</Project>